use mta;
create table ccdmfilter (time FLOAT,   CCSDSTMF CHAR,       COBSRQID BIGINT,       COGEOMTR CHAR,       SCIINS CHAR, 3FAPOS FLOAT, 4HPOSARO FLOAT,       4LPOSARO FLOAT,       GRATING CHAR, AOPCADMD CHAR,       CORADMEN CHAR);
load data local infile '/data/mta/DataSeeker/data/repository/ccdmfilter.rdb' into table ccdmfilter fields terminated by '\t' lines terminated by '\n';
create table pcadfilter (time FLOAT,   OATTRAS FLOAT,       AOATTDEC FLOAT,       AOATTROL FLOAT);
load data local infile '/data/mta/DataSeeker/data/repository/pcadfilter.rdb' into table pcadfilter fields terminated by '\t' lines terminated by '\n';
create table deahk_temp (time FLOAT,   Navg INT,   DEAHK1_avg FLOAT,     DEAHK1_s FLOAT,       DEAHK2_avg FLOAT,     DEAHK2_s FLOAT,       DEAHK3_avg FLOAT,     DEAHK3_s FLOAT,       DEAHK4_avg FLOAT,     DEAHK4_s FLOAT,    DEAHK5_avg FLOAT,     DEAHK5_s FLOAT,       DEAHK6_avg FLOAT,     DEAHK6_s FLOAT,       DEAHK7_avg FLOAT,     DEAHK7_s FLOAT,       DEAHK8_avg FLOAT,     DEAHK8_s FLOAT,   DEAHK9_avg FLOAT,      DEAHK9_s FLOAT,       DEAHK10_avg FLOAT,    DEAHK10_s FLOAT,      DEAHK11_avg FLOAT,    DEAHK11_s FLOAT,      DEAHK12_avg FLOAT,    DEAHK12_s FLOAT,      DEAHK13_avg FLOAT, DEAHK13_s FLOAT,      DEAHK15_avg FLOAT,    DEAHK15_s FLOAT,      DEAHK16_avg FLOAT,    DEAHK16_s FLOAT);
load data local infile '/data/mta/DataSeeker/data/repository/deahk_temp.rdb' into table deahk_temp fields terminated by '\t' lines terminated by '\n';
create table deahk_elec (time FLOAT,   Navg INT,   DEAHK17_avg FLOAT,    DEAHK17_s FLOAT,      DEAHK18_avg FLOAT,    DEAHK18_s FLOAT,      DEAHK19_avg FLOAT,    DEAHK19_s FLOAT,      DEAHK20_avg FLOAT,    DEAHK20_s FLOAT,   DEAHK25_avg FLOAT,    DEAHK25_s FLOAT,      DEAHK26_avg FLOAT,    DEAHK26_s FLOAT,      DEAHK27_avg FLOAT,    DEAHK27_s FLOAT,      DEAHK28_avg FLOAT,    DEAHK28_s FLOAT,  DEAHK29_avg FLOAT,     DEAHK29_s FLOAT,      DEAHK30_avg FLOAT,    DEAHK30_s FLOAT,      DEAHK31_avg FLOAT,    DEAHK31_s FLOAT,      DEAHK32_avg FLOAT,    DEAHK32_s FLOAT,      DEAHK33_avg FLOAT, DEAHK33_s FLOAT,      DEAHK34_avg FLOAT,    DEAHK34_s FLOAT,      DEAHK35_avg FLOAT,    DEAHK35_s FLOAT,      DEAHK36_avg FLOAT,    DEAHK36_s FLOAT,      DEAHK37_avg FLOAT,DEAHK37_s FLOAT,       DEAHK38_avg FLOAT,    DEAHK38_s FLOAT,      DEAHK39_avg FLOAT,    DEAHK39_s FLOAT,      DEAHK40_avg FLOAT,    DEAHK40_s FLOAT);
load data local infile '/data/mta/DataSeeker/data/repository/deahk_elec.rdb' into table deahk_elec fields terminated by '\t' lines terminated by '\n';
show table status;
